{
    "name": "nerdic-payload-mcp",
    "version": "1.0.0",
    "description": "Payload MCP tool for generating payload specific structures",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsup src/server.ts --format cjs,esm --dts",
        "start": "node dist/server.js",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest"
    },
    "keywords": [
        "payloadcms",
        "mcp",
        "code-generation",
        "validation",
        "ai",
        "agent"
    ],
    "author": "Your Name",
    "license": "ISC",
    "dependencies": {
        "express": "^4.21.2",
        "dotenv": "^16.4.7",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "openai": "^4.90.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.17.28",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "prettier": "^3.5.3",
        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "typescript": "^5.3.2"
    },
    "packageManager": "pnpm@10.6.3"
}
